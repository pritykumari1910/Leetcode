class Solution {

public:

    int countTriples(int n) {

        int cnt = 0;

        for(int a = 1; a < n; a++){
            for(int b = a + 1; b < n; b++){
                double c = sqrt(a*a + b*b);
                if(c <= n && c == (int)c) cnt += 2;
            }
        }
        return cnt;
    }
};
