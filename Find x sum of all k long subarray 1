if(height.size() == 1) return 0;
        int first = 0;
        int second = abs(height[1]-height[0]);
        for(int i = 2;i<height.size();i++){
            int s = second+abs(height[i]-height[i-1]);
            int f = first+abs(height[i]-height[i-2]);
            first = second;
            second = min(f,s);
        }
        return second;
