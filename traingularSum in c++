class Solution {
public:
    int triangularSum(vector<int>& nums) {
        int n = nums.size();
        while(n!=1){
            for(int i=0;i<n-1;i++){
                nums[i]=(nums[i]+nums[i+1])%10;
                if(i==n-2){
                    nums.erase(nums.begin()+i+1,nums.begin()+i+2);
                }
            }
            n--;

        }

return nums[0];

        
    }
};
